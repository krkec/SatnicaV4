<Window x:Class="SatnicaV3.kartica"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="kartica" Height="355" Width="552">
    <Grid Margin="0,0,2,13">
        <DataGrid AutoGenerateColumns="False" Name="grdKartica" Margin="10,10,50,24" ScrollViewer.HorizontalScrollBarVisibility="Hidden" ScrollViewer.VerticalScrollBarVisibility="Hidden" Loaded="grdKartica_Loaded" >

            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding RZ }" IsReadOnly="True" Width="50"  Header="RZ"/>
                <DataGridTextColumn Binding="{Binding Tjedan1}"  IsReadOnly="True" Width="60" Header="1 Tjedan"/>
                <DataGridTextColumn Binding="{Binding Tjedan2}" Width="60" Header="2 Tjedan"/>
                <DataGridTextColumn Binding="{Binding Tjedan3}" Width="60" Header="3 Tjedan"/>
                <DataGridTextColumn Binding="{Binding Tjedan4}" Width="60" Header="4 Tjedan"/>
                <DataGridTextColumn Binding="{Binding Tjedan5}" Width="60" Header="5 Tjedan"/>
                <DataGridTextColumn Binding="{Binding Tjedan6}" Width="60" Header="6 Tjedan"/>
                <DataGridTextColumn Binding="{Binding UKUPNO}" Width="70" Header="Ukupno"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
